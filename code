pragma solidity ^0.8.7;

contract hell{
    uint a;
    uint b;
    uint c;
    uint d;


    function set(uint _a,uint _b,uint _c,uint _d) public {
        a=_a;
        b=_b;
        c=_c;
        d=_d;
    }

    function get() view public returns (uint f){
        return a+b+c+d;
    }

}

